{
  "version": "0.0.1",
  "description": "Articleman, risen from the embers.",
  "name": "articleman-backend",
  "scripts": {
    "build": "webpack",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test"
  },
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.23.0",
    "@shelf/fast-chunk-string": "^3.0.0",
    "@types/google-apps-script": "^1.0.64",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "babel-plugin-import": "^1.13.8",
    "babel-plugin-inline-json-import": "^0.3.2",
    "babel-plugin-tsconfig-paths": "^1.0.3",
    "banner-plugin": "^1.0.10",
    "circular-dependency-plugin": "^5.2.2",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-googleappsscript": "^1.0.4",
    "eslint-plugin-tsdoc": "^0.2.17",
    "gas-webpack-plugin": "^2.4.0",
    "prettier": "^2.8.8",
    "qunit": "^2.19.4",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.4.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@abraham/reflection": "^0.12.0",
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "@types/lodash": "^4.14.197",
    "babel-loader": "^9.1.3",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "compress-json": "^2.1.2",
    "inversify": "^6.0.1",
    "inversify-global-container": "^0.5.2",
    "lodash": "^4.17.21",
    "lodash-webpack-plugin": "^0.11.6",
    "uuid": "^9.0.0",
    "worktapper": "^1.1.1"
  },
  "private": true,
  "repository": "",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  }
}
